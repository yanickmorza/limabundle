{
    "description": "Light Interface Maker Application",
    "homepage": "https://github.com/yanickmorza/limabundle",
    "name": "yanickmorza/limabundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["generator", "code generator", "scaffolding", "scaffold"],
    "authors": [
        {
            "name": "Yanick Morza",
            "email": "yanick.morza@free.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.5"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\LimaBundle\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@InstallService"
        ],
        "InstallService": "@php vendor/yanickmorza/limabundle/src/Command/installServiceLima.php"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
