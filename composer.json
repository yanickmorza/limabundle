{
    "name": "yanickmorza/limabundle",
    "type": "symfony-bundle",
    "description": "Light Interface Maker Application",
    "homepage": "https://github.com/yanickmorza/limabundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Yanick Morza"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/swiftmailer-bundle": "^3.5"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\LimaBundle\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "composer-scripts": {
                "make vendor/yanickmorza/limabundle/src/Command/installation": "script"
            },
            "installation": [
                "@php vendor/yanickmorza/limabundle/src/Command/installation.php"
            ],
            "post-install-cmd": [
                "@auto-scripts",
                "@composer installation"
            ],
            "post-update-cmd": [
                "@auto-scripts",
                "@composer installation"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.9.x-dev"
        }
    }
}